{
  "name": "mmpay-browser-sdk",
  "version": "1.0.2",
  "description": "JavaScript SDK for integrating the MMQR Merchant and Redirect payment gateway",
  "keywords": [
    "MMQR",
    "Master",
    "Visa",
    "JCB",
    "MPU"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nawing/Myan-Myan-Pay-SDK.git"
  },
  "bugs": {
    "url": "https://github.com/nawing/Myan-Myan-Pay-SDK/issues"
  },
  "homepage": "https://github.com/nawing/Myan-Myan-Pay-SDK#readme",
  "scripts": {
    "build": "npm run clean && tsc && tsc -p tsconfig.cjs.json",
    "clean": "rm -rf dist",
    "dev": "tsc --watch"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.10.6",
    "typescript": "^5.9.3"
  },
  "author": "Naw Ing",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.2",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.2"
  }
}
